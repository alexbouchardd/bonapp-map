{
  "name": "bonapp",
  "version": "1.0.0",
  "description": "Bonapp network map",
  "main": "app.js",
  "engines": {
    "node": "8.9.1"
  },
  "scripts": {
    "dev:start": "./node_modules/.bin/babel-node ./src/server",
    "dev:watch": "concurrently -k -r \"babel-watch src/server/index.js\" \"npm run build -- --watch\"",
    "dev:build": "webpack",
    "build": "./node_modules/.bin/babel-node scripts/build-files | NODE_ENV=production npm run postinstall",
    "postinstall": "webpack -p --config ./webpack.config.js --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexbouchardd/bonapp.git"
  },
  "author": "Alexandre Bouchard",
  "bugs": {
    "url": "https://github.com/alexbouchardd/bonapp/issues"
  },
  "homepage": "https://github.com/alexbouchardd/bonapp#readme",
  "dependencies": {
    "babel-cli": "^6.5.2",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "body-parser": "^1.15.2",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "google-map-react": "^0.25.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-pure-render": "^1.0.2"
  },
  "devDependencies": {
    "json-loader": "^0.5.4",
    "webpack": "^3.8.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "babel-watch": "^2.0.3",
    "concurrently": "^3.1.0"
  }
}
