{
  "name": "bonapp",
  "version": "1.0.0",
  "description": "Bonapjp network platform",
  "main": "app.js",
  "scripts": {
    "start": "node ./src/server",
    "watch": "concurrently -k -r 'nodemon ./src/server --exec babel-node' 'npm run build -- --watch'",
    "build": "webpack src/client public/app.js --module-bind 'js=babel-loader'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run build && babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexbouchardd/bonapp.git"
  },
  "author": "Bonapp",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexbouchardd/bonapp/issues"
  },
  "homepage": "https://github.com/alexbouchardd/bonapp#readme",
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "request-promise": "^4.1.1",
    "superagent": "^2.3.0",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.16.0",
    "concurrently": "^3.1.0",
    "nodemon": "^1.11.0"
  }
}
