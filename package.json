  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexbouchardd/bonapp/issues"
  },
  "homepage": "https://github.com/alexbouchardd/bonapp#readme",
  "dependencies": {
    "babel-cli": "^6.5.2",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.15.2",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "google-maps-react": "^1.0.19",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-pure-render": "^1.0.2",
    "webpack": "^1.13.2"
