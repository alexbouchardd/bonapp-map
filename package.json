{
  "name": "bonapp",
  "version": "1.0.0",
  "description": "Bonapp network platform",
  "main": "app.js",
  "engines": {
    "node": "6.6.0"
  },
  "scripts": {
    "start": "node ./src/server",
    "watch": "concurrently -k -r \"babel-watch src/server/index.js\" \"npm run build -- --watch\"",
    "build": "webpack",
    "webpack:production": "webpack -p",
    "postinstall": "npm run build && npm run start"
  },
